    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "jsx": "react",
    "jsxFactory": "JSX.createElement",
    "jsxFragmentFactory": "JSX.Fragment"
},
"include": ["src"]
}